@model AsyncInn.Models.RoomAmenity

@{
  ViewData["Title"] = "Delete";
}
<head>
  <link href="~/StyleSheet.css" rel="stylesheet" />
</head>



<h2>Delete</h2>

<h3>Are you sure you want to delete this Room Amenity?</h3>
<div>
  <dl class="dl-horizontal">
    <dt>
      @Html.DisplayNameFor(model => model.Room): @Html.DisplayFor(model => model.Room.Name)
    </dt>
    <dt>
      @Html.DisplayNameFor(model => model.Amenity): @Html.DisplayFor(model => model.Amenity.Name)
    </dt>
  </dl>

  <form class="row" asp-action="Delete">
    <input type="hidden" asp-for="AmenityID" />
    <input type="hidden" asp-for="RoomID" />
    <input type="submit" value="Delete" class="btn btn-default" />
    <a asp-action="Index">Back to List</a>
  </form>
</div>
